{
    "name": "idrinth/yaml-facade",
    "description": "provides an facade for php-yaml, syck and symphony/yaml so switching in between is easier",
    "homepage": "https://github.com/idrinth/php-yaml-facade/",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "facade",
        "yaml",
        "yml",
        "serializer",
        "deserializer"
    ],
    "authors" : [{
        "name": "Björn Büttner",
        "homepage": "https://github.com/idrinth/"
    }],
    "require": {
        "php": "^8.3",
        "symfony/yaml": ">=2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3 || ^12.0.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "suggest": {
        "ext-yaml": "The most up to date, faster option for parsing and writing yaml",
        "ext-syck": "A faster option for parsing and writing yaml"
    },
    "autoload": {
        "psr-4": {
             "De\\Idrinth\\Yaml\\": "src" 
        }
    },
    "autoload-dev": {
        "psr-4": {
             "De\\Idrinth\\Yaml\\Test\\": "test" 
        }
    }
}
